using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int[] arr = new int[5];
        Console.WriteLine(arr.Length);
        for(int i = 0 ; i< arr.Length; i++){
            arr[i] = i+1;
        }
        for(int i = 0 ; i< arr.Length; i++){
            Console.WriteLine("arr[{0}] = {1}", i, arr[i]);
        }
    }
}
